
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @default(autoincrement()) @id
  email     String   @unique
  username      String
  firstName String?
  lastName  String?
  passHash  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Bookmarks {
  id        Int      @default(autoincrement()) @id
  url       String
  title     String
  description String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  userId    Int
  // User      User     @relation(fields: [userId], references: [id])
}


